#include <stdio.h>

int main() {
    int list1[50], list2[50], merged[100];
    int n1, n2, i, j;

    // Input first list
    printf("Enter size of first list: ");
    scanf("%d", &n1);
    printf("Enter elements of first list:\n");
    for(i = 0; i < n1; i++) {
        scanf("%d", &list1[i]);
    }

    // Input second list
    printf("Enter size of second list: ");
    scanf("%d", &n2);
    printf("Enter elements of second list:\n");
    for(i = 0; i < n2; i++) {
        scanf("%d", &list2[i]);
    }

    // Merge lists
    for(i = 0; i < n1; i++) {
        merged[i] = list1[i];
    }
    for(j = 0; j < n2; j++) {
        merged[i] = list2[j];
        i++;
    }

    // Print merged list
    printf("Merged List: ");
    for(i = 0; i < n1 + n2; i++) {
        printf("%d ", merged[i]);
    }

    return 0;
}
