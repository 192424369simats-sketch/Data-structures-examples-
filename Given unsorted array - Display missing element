#include <stdio.h>

int main() {
    int arr[50], n, i, total, sum = 0, missing;

    printf("Enter the size of array (n-1 elements, one missing): ");
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }

    int max = arr[0];
    for(i = 1; i < n; i++) {
        if(arr[i] > max)
            max = arr[i];
    }

    total = (max * (max + 1)) / 2;  // Expected sum from 1 to max
    missing = total - sum;

    if(missing == 0)
        printf("No missing element detected (check input).\n");
    else
        printf("Missing element is: %d\n", missing);

    return 0;
}
