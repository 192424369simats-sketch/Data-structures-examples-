#include <stdio.h>

int main() {
    int arr1[50], arr2[50], arr3[100];
    int n1, n2, n3, i;

    // Input first array
    printf("Enter size of first array: ");
    scanf("%d", &n1);
    printf("Enter %d elements for first array:\n", n1);
    for(i = 0; i < n1; i++) {
        scanf("%d", &arr1[i]);
    }

    // Input second array
    printf("Enter size of second array: ");
    scanf("%d", &n2);
    printf("Enter %d elements for second array:\n", n2);
    for(i = 0; i < n2; i++) {
        scanf("%d", &arr2[i]);
    }

    // Concatenate arrays
    n3 = n1 + n2;
    for(i = 0; i < n1; i++) {
        arr3[i] = arr1[i];
    }
    for(i = 0; i < n2; i++) {
        arr3[n1 + i] = arr2[i];
    }

    // Display result
    printf("\nConcatenated Array: ");
    for(i = 0; i < n3; i++) {
        printf("%d ", arr3[i]);
    }

    return 0;
}
