#include <stdio.h>

int main() {
    int reg[50], n, i, key, found = 0;

    printf("Enter number of students: ");
    scanf("%d", &n);

    printf("Enter %d registration numbers:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &reg[i]);
    }

    printf("Enter registration number to search: ");
    scanf("%d", &key);

    for(i = 0; i < n; i++) {
        if(reg[i] == key) {
            printf("Registration number %d found at position %d\n", key, i+1);
            found = 1;
            break;
        }
    }

    if(!found) {
        printf("Registration number %d not found\n", key);
    }

    return 0;
}
